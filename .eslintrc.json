{
  "extends": [
    "@rocketseat/eslint-config/node"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto",
        "semi": false,
        "singleQuote": true
      }
    ],
    "no-new": "off",
    "camelcase": "off",
    "no-useless-constructor": "off"
  }
}